    "concurrently": "^8.2.2",
    "date-fns": "^3.3.1",
    "dexie": "^3.2.5",
    "express": "^4.18.2",
    "graphql": "^16.12.0",
    "react-datepicker": "^6.1.0",
    "react-router": "^6.22.0",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "nodemon": "^3.0.3"
  }
